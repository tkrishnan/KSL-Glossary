<h1>HI</h1>

<div class="pure-g">
  <div class="pure-u-3-4">
    <table class="pure-table">
        <thead>
            <tr>
                <th>Word</th>
                <th>Title</th>
                <th>Categories</th>
                <th>URL</th>
            </tr>
        </thead>

        <tbody>
            {{#each vids}}
            <tr class="table-row" data-row="{{ @index}}">
                <td>{{word}}</td>
                <td>{{title}}</td>
                <td>{{categories}}</td>
                <td>{{url}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
  </div>

  <div class="pure-u-1-4">
    <form class="pure-form">
      <fieldset>
        <legend>test</legend>

        <label for="admin-word">Word</label>
        <input type="text" id="admin-word" placeholder="Word">

        <label for="admin-title">Title</label>
        <input type="text" id="admin-title" placeholder="Title">

        <label for="admin-categories">Categories (Comma Separated)</label>
        <input type="text" id="admin-categories" placeholder="i.e. biology,physics,math">

        <label for"admin-file">Upload File</label>
        <input type="" id="admin-categories" placeholder="Select File">

        <input type="submit" class="pure-button" value="Upload">
      </fieldset>
    </form>
  </div>
</div>